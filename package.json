{
  "name": "storable-react",
  "version": "0.0.1",
  "description": "React bindings for the Storable library",
  "main": "index.js",
  "scripts": {
    "compile": "babel -i __tests__ -d ./ src/",
    "prepublish": "npm run compile && npm test",
    "test": "jest"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "../node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "dependencies": {
    "react": "^0.13.3",
    "storablejs": "0.0.6"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-jest": "^5.3.0",
    "jest-cli": "^0.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nramadas/Storable-React.git"
  },
  "keywords": [
    "storable",
    "rx",
    "rxjs",
    "react",
    "observable",
    "datastore"
  ],
  "author": "nramadas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nramadas/Storable-React/issues"
  },
  "homepage": "https://github.com/nramadas/Storable-React#readme"
}
